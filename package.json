  "license": "Apache-2.0",
  "description": "Material Components Web for Angular",
  "author": "The Source Zone",
  "bugs": "https://bitbucket.org/src-zone/material/issues",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/src-zone/material.git"
  },
  "homepage": "https://bitbucket.org/src-zone/material",
  "main": "dist/material.umd.js",
  "module": "dist/material.es5.js",
  "types": "dist/material.d.ts",
  "keywords": [
    "angular",
    "material",
    "material components web"
  ],
  "scripts": {
    "clean": "rimraf build dist",
    "ngc": "ngc -p tsconfig.json",
    "rollup": "rollup -c",
    "copy-types": "cpy **/*.d.ts ../dist --cwd=build --parents",
    "copy-metadata": "cpy build/material.metadata.json dist/",
    "build": "npm run clean && npm run ngc && npm run rollup && npm run copy-types && npm run copy-metadata"
  },
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "material-components-web": "^0.10.0",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.1.3",
    "@ngtools/webpack": "^1.2.12",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.21",
    "@types/selenium-webdriver": "^3.0.4",
    "babel-core": "^6.24.1",
