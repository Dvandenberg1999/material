{
  "name": "@blox/material",
  "version": "0.1.3",
  "license": "Apache-2.0",
  "description": "Material Components for Angular",
  "author": "The Source Zone",
  "bugs": "https://bitbucket.org/src-zone/material/issues",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/src-zone/material.git"
  },
  "homepage": "http://blox.src.zone/material",
  "main": "dist/material.umd.js",
  "module": "dist/material.es5.js",
  "types": "dist/material.d.ts",
  "keywords": [
    "angular",
    "material",
    "material components web"
  ],
  "scripts": {
    "clean": "rimraf build dist",
    "ngc": "ngc -p tsconfig.json",
    "rollup": "rollup -c",
    "copy-types": "cpy **/*.d.ts ../dist --cwd=build --parents",
    "copy-metadata": "cpy build/material.metadata.json dist/",
    "build": "npm run clean && npm run ngc && npm run rollup && npm run copy-types && npm run copy-metadata",
    "link2site": "rimraf ../site/node_modules/@blox/material/*  && cpy package.json dist/**/*.* src/**/*.* ../site/node_modules/@blox/material/ --parents"
  },
  "dependencies": {
    "material-components-web": ">=0.13.0"
  },
  "peerDependencies": {
    "@angular/common": ">=4.1.3",
    "@angular/core": ">=4.1.3",
    "@angular/forms": ">=4.1.3",
    "rxjs": ">=5.4.0",
    "zone.js": ">=0.8.11"
  },
  "devDependencies": {
    "@angular/common": "^4.4.1",
    "@angular/compiler": "^4.4.1",
    "@angular/compiler-cli": "^4.4.1",
    "@angular/core": "^4.4.1",
    "@angular/forms": "^4.4.1",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.28",
    "@types/selenium-webdriver": "^3.0.6",
    "codelyzer": "^3.2.0",
    "cpy-cli": "^1.0.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-remap-istanbul": "0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "remap-istanbul": "^0.9.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.49.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rxjs": "^5.4.3",
    "shelljs": "^0.7.8",
    "source-map-explorer": "^1.4.0",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.7.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.5.2",
    "zone.js": "^0.8.17"
  }
}
